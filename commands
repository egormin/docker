sudo docker build -t nginx -f web.Dockerfile .
sudo docker build -t tomcat -f tomcat.Dockerfile .
sudo docker build -t application -f application.Dockerfile .
sudo docker run -d --name appvolume applicationsudo 
docker run -d --volumes-from appvolume --name tomcat tomcatsudo 
docker run --link tomcat:tomcat --name nginx -d -p 8080:80 nginx